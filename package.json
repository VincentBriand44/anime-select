{
  "name": "anime-select",
  "version": "0.0.1",
  "private": false,
  "author": {
    "email": "pro.vincent.briand@pm.me",
    "name": "Vincent BRIAND"
  },
  "license": "MIT",
  "description": "Website to select anime for the new season",
  "repository": {
    "type": "git",
    "url": "https://github.com/VincentBriand44/anime-select"
  },
  "scripts": {
    "dev": "vite dev",
    "build": "vite build",
    "preview": "vite preview",
    "check": "bunx @biomejs/biome check --write . && svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "bunx @biomejs/biome check --write . && svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "lint": "bunx @biomejs/biome lint --write .",
    "format": "bunx @biomejs/biome format --write ."
  },
  "devDependencies": {
    "@biomejs/biome": "1.9.0",
    "@sveltejs/adapter-auto": "3.2.4",
    "@sveltejs/kit": "2.5.27",
    "@sveltejs/vite-plugin-svelte": "3.1.2",
    "autoprefixer": "10.4.20",
    "commitizen": "4.3.0",
    "cz-emoji": "1.3.2-canary.2",
    "jikan4.js": "1.6.0",
    "postcss": "8.4.47",
    "svelte": "4.2.19",
    "svelte-check": "4.0.2",
    "tailwindcss": "3.4.11",
    "typescript": "5.6.2",
    "valibot": "0.42.0",
    "vite": "5.4.5"
  },
  "type": "module",
  "config": {
    "commitizen": {
      "path": "cz-emoji"
    },
    "cz-emoji": {
      "skipQuestions": ["scope", "body", "issues", "breaking"]
    }
  },
  "trustedDependencies": ["@biomejs/biome"]
}
